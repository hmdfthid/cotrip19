<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cotrip.cotripjfx.UserLogin">
   <children>
      <Label layoutX="296.0" layoutY="152.0" text="You are Logging In as a tourist">
         <VBox.margin>
            <Insets bottom="12.0" />
         </VBox.margin></Label>

      <TextField fx:id="usernameField" promptText="Username">
         <VBox.margin>
            <Insets bottom="4.0" />
         </VBox.margin></TextField>
      <PasswordField fx:id="passwordField" promptText="Password">
         <VBox.margin>
            <Insets bottom="4.0" />
         </VBox.margin>
      </PasswordField>
      <Button mnemonicParsing="false" onAction="#login" text="Login">
         <VBox.margin>
            <Insets bottom="4.0" />
         </VBox.margin></Button>
      <Button mnemonicParsing="false" onAction="#usercreate" text="Create Account">
         <VBox.margin>
            <Insets bottom="4.0" />
         </VBox.margin></Button>
      <Button mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
